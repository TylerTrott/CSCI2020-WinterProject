<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tictactoe.TicTacToeController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <TextField fx:id="signUpusername" text="Enter a Username" />  <!--Username (a string) -->
    <TextField fx:id="signUppassword" text="Enter a Password" />  <!--Password (a string)  -->
    <Button fx:id="submitBtn" onAction="#onSubmitClick" text="Submit" />
    <Button fx:id="returnToMain" onAction="#onSignupReturnClick" text="Return to Main" />
</VBox>
