<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" spacing="20.0" style="-fx-background-color: #ffffff;" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tictactoe.TicTacToeController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Choose game type:">
        <font>
            <Font name="Calibri Bold" size="23.0" />
        </font>
    </Text>
    <Button fx:id="localBtn" onAction="#onLocalClick" text="Local Play">
        <font>
            <Font name="Calibri" size="14.0" />
        </font></Button>
    <Button fx:id="multiplayerBtn" onAction="#onMultiplayerClick" text="Multiplayer">
        <font>
            <Font name="Calibri" size="14.0" />
        </font></Button>
    <Button fx:id="returnToMain" onAction="#onChooseGameReturnClick" text="Return to Main">
        <font>
            <Font name="Calibri" size="14.0" />
        </font></Button>
</VBox>
