<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" prefHeight="236.0" prefWidth="208.0" spacing="20.0" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tictactoe.TicTacToeController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <Text strokeType="OUTSIDE" strokeWidth="0.0" text="TicTacToe">
      <font>
         <Font name="Calibri Bold" size="30.0" />
      </font>
   </Text>

    <Button fx:id="firstLoginBtn" onAction="#onLoginButtonClick" text="Login" />
   <Button fx:id="signUpBtn" onAction="#onSignUpButtonClick" text="Sign Up" />
   <Button fx:id="MainMenuExitBtn" onAction="#onExitButtonClick" text="Exit" />

</VBox>
